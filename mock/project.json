{
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "name": "mock",
  "projectType": "application",
  "sourceRoot": "apps/mock/src",
  "tags": [
    "astro:mock"
  ],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec astro --root apps/mock build"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "dependsOn": [
        "build"
      ],
      "options": {
        "commands": [
          "pnpm exec astro --root apps/mock preview"
        ]
      }
    },
    "add": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec astro --root apps/mock add"
      }
    },
    "info": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec astro --root apps/mock info"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec astro --root apps/mock check"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm exec astro --root apps/mock dev"
      }
    }
  }
}